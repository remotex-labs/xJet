{
    "name": "@remotex-labs/xjet",
    "type": "module",
    "main": "dist/index.js",
    "types": "./dist/index.d.ts",
    "module": "./dist/index.js",
    "author": "Garefild",
    "version": "1.0.0-local",
    "license": "Mozilla Public License Version 2.0",
    "description": "xJet: Run Automated Tests Anywhere, Effortlessly",
    "homepage": "https://remotex-labs.github.io/xJet/",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/remotex-labs/xJet.git"
    },
    "bugs": {
        "url": "https://github.com/remotex-labs/xJet/issues"
    },
    "keywords": [
        "jet",
        "xjet",
        "test",
        "jest",
        "mocha",
        "watch",
        "testing",
        "mocking",
        "jasmine",
        "coverage",
        "typescript"
    ],
    "engines": {
        "node": ">=20"
    },
    "files": [
        "dist",
        "LICENSE",
        "README.md"
    ],
    "bin": {
        "jet": "./dist/bash.js",
        "xjet": "./dist/bash.js",
        "xJet": "./dist/bash.js"
    },
    "scripts": {
        "dev": "xbuild -w",
        "build": "xbuild",
        "test": "jest",
        "test:coverage": "jest --coverage",
        "preci:test": "xbuild",
        "lint": "npm run lint:md && npm run lint:eslint && npm run lint:typescript",
        "lint:md": "markdownlint \"**/*.md\" \".github/**/*.md\" --ignore \"node_modules/**\"",
        "lint:eslint": "eslint . -c ./eslint.config.mjs",
        "lint:typescript": "xbuild --tc",
        "ci:test": "jest",
        "ci:clean": "rm -rf ./dist",
        "ci:build": "npm run build",
        "docs:dev": "vitepress dev docs",
        "docs:build": "vitepress build docs",
        "docs:preview": "vitepress preview docs"
    },
    "devDependencies": {
        "jest": "^30.2.0",
        "eslint": "^9.37.0",
        "vitepress": "^2.0.0-alpha.12",
        "markdownlint-cli": "^0.45.0",
        "typescript-eslint": "^8.45.0",
        "eslint-plugin-tsdoc": "^0.4.0",
        "eslint-plugin-perfectionist": "^4.15.0",
        "@swc/jest": "^0.2.39",
        "@types/jest": "^30.0.0",
        "@types/node": "^24.6.2",
        "@viteplus/versions": "^2.0.2",
        "@remotex-labs/xbuild": "1.6.0"
    },
    "dependencies": {
        "yargs": "^18.0.0",
        "esbuild": "^0.25.9",
        "typescript": "^5.9.3",
        "@remotex-labs/xmap": "^4.0.2",
        "@remotex-labs/xansi": "^1.3.3",
        "@remotex-labs/xstruct": "^2.0.0",
        "@remotex-labs/xjet-expect": "^2.1.2"
    }
}
